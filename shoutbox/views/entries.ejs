<!DOCTYPE html>
<html>
    <head>
        <title><%= title %></title>
        <link rel='stylesheet' href='/stylesheets/style.css' />
    </head>
    <body>
        <p>Menu placeholder</p>
        <% entries.forEach((entry) => { %>
            <div class='entry'>
                <h3><%= entry.title %></h3>
                <p><%= entry.body %></p>
                <p>Posted by <%= entry.usernmae %></p>
            </div>
        <% }) %>
    </body>
</html>